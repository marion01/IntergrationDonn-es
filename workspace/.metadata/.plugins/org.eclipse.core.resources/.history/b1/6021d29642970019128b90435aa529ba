      <indicators xsi:type="dataquality.indicators:UniqueCountIndicator" xmi:id="_ki3HhJdCEemwqt0XGhOv7A">
        <parameters xmi:id="_ki3HhZdCEemwqt0XGhOv7A"/>
        <analyzedElement xsi:type="relational:TdColumn" href="../../metadata/connections/EDD_0.1.item#_lpM1-oBbEemyxqr3WvsPQg"/>
