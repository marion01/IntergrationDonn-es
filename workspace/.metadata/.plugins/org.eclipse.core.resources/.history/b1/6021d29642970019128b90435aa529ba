        <builtInIndicatorDefinition xmi:id="_lkqh4JdCEemwqt0XGhOv7A" name="Null Count" label="Null Count">
          <taggedValue xmi:id="_lkqh4ZdCEemwqt0XGhOv7A" tag="Purpose" value="evaluate the number of missing data (null records)"/>
          <taggedValue xmi:id="_lkqh4pdCEemwqt0XGhOv7A" tag="Description" value="counts the number of null rows"/>
          <sqlGenericExpression xmi:id="_lkqh45dCEemwqt0XGhOv7A" body="SELECT COUNT(*) FROM &lt;%=__TABLE_NAME__%> WHERE &lt;%=__COLUMN_NAMES__%> IS NULL &lt;%=__AND_WHERE_CLAUSE__%>" language="SQL"/>
        </builtInIndicatorDefinition>
      </indicators>
      <indicators xsi:type="dataquality.indicators:DistinctCountIndicator" xmi:id="_ki3Hv5dCEemwqt0XGhOv7A">
        <parameters xmi:id="_ki3HwJdCEemwqt0XGhOv7A"/>
        <analyzedElement xsi:type="relational:TdColumn" href="../../metadata/connections/EDD_0.1.item#_lpM16oBbEemyxqr3WvsPQg"/>
        <indicatorDefinition href="../../TDQ_Libraries/Indicators/System Indicators/Simple Statistics/Distinct_Count_0.1.definition#_ccHq0BF2Ed2PKb6nEJEvhw"/>
        <builtInIndicatorDefinition xmi:id="_lk9c0JdCEemwqt0XGhOv7A" name="Distinct Count" label="Distinct Count">
          <taggedValue xmi:id="_lk9c0ZdCEemwqt0XGhOv7A" tag="Description" value="counts the number of distinct rows (like a &quot;SELECT DISTINCT&quot; SQL statement)"/>
          <taggedValue xmi:id="_lk9c0pdCEemwqt0XGhOv7A" tag="Purpose" value="evaluates the number of distinct records"/>
          <sqlGenericExpression xmi:id="_lk9c05dCEemwqt0XGhOv7A" body="SELECT COUNT(*) FROM (SELECT DISTINCT &lt;%=__COLUMN_NAMES__%> FROM &lt;%=__TABLE_NAME__%> &lt;%=__WHERE_CLAUSE__%>) A" language="SQL"/>
        </builtInIndicatorDefinition>
      </indicators>
      <indicators xsi:type="dataquality.indicators:UniqueCountIndicator" xmi:id="_ki3HwZdCEemwqt0XGhOv7A">
        <parameters xmi:id="_ki3HwpdCEemwqt0XGhOv7A"/>
        <analyzedElement xsi:type="relational:TdColumn" href="../../metadata/connections/EDD_0.1.item#_lpM16oBbEemyxqr3WvsPQg"/>
        <indicatorDefinition href="../../TDQ_Libraries/Indicators/System Indicators/Simple Statistics/Unique_Count_0.1.definition#_ccHq0RF2Ed2PKb6nEJEvhw"/>
        <builtInIndicatorDefinition xmi:id="_llHN0JdCEemwqt0XGhOv7A" name="Unique Count" label="Unique Count">
          <taggedValue xmi:id="_llHN0ZdCEemwqt0XGhOv7A" tag="Description" value="counts the number of unique rows (i.e. distinct rows with only one instance: rows that are not duplicated)"/>
          <taggedValue xmi:id="_llHN0pdCEemwqt0XGhOv7A" tag="Purpose" value="evaluates the number of unique records"/>
          <sqlGenericExpression xmi:id="_llHN05dCEemwqt0XGhOv7A" body="SELECT COUNT(*) FROM (SELECT &lt;%=__COLUMN_NAMES__%>, COUNT(*) FROM &lt;%=__TABLE_NAME__%> &lt;%=__WHERE_CLAUSE__%> GROUP BY &lt;%=__COLUMN_NAMES__%> HAVING COUNT(*) = 1) AS myquery" language="SQL"/>
