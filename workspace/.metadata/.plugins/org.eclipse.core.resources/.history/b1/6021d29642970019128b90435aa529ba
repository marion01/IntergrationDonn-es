      <indicators xsi:type="dataquality.indicators:BlankCountIndicator" xmi:id="_ki3HS5dCEemwqt0XGhOv7A">
        <parameters xmi:id="_ki3HTJdCEemwqt0XGhOv7A"/>
        <analyzedElement xsi:type="relational:TdColumn" href="../../metadata/connections/EDD_0.1.item#_lpM19oBbEemyxqr3WvsPQg"/>
        <indicatorDefinition href="../../TDQ_Libraries/Indicators/System Indicators/Simple Statistics/Blank_Count_0.1.definition#_ccHq0xF2Ed2PKb6nEJEvhw"/>
        <builtInIndicatorDefinition xmi:id="_la_BcJdCEemwqt0XGhOv7A" name="Blank Count" label="Blank Count">
          <taggedValue xmi:id="_la_BcZdCEemwqt0XGhOv7A" tag="Description" value="counts the number of blank rows. A blank is a non null textual data that contains only white spaces (or nothing). Note that Oracle does not distinguish between the empty string and the null value"/>
          <taggedValue xmi:id="_la_BcpdCEemwqt0XGhOv7A" tag="Purpose" value="evaluates the number of blank records"/>
          <sqlGenericExpression xmi:id="_la_Bc5dCEemwqt0XGhOv7A" body="SELECT COUNT(&lt;%=__COLUMN_NAMES__%>) FROM &lt;%=__TABLE_NAME__%> WHERE TRIM(&lt;%=__COLUMN_NAMES__%>) = '' &lt;%=__AND_WHERE_CLAUSE__%>" language="SQL"/>
          <sqlGenericExpression xmi:id="_la_BdJdCEemwqt0XGhOv7A" body="SELECT COUNT(&lt;%=__COLUMN_NAMES__%>) FROM &lt;%=__TABLE_NAME__%>  WHERE TRIM(&lt;%=__COLUMN_NAMES__%>) IS NULL &lt;%=__AND_WHERE_CLAUSE__%>" language="Oracle"/>
          <sqlGenericExpression xmi:id="_la_BdZdCEemwqt0XGhOv7A" body="SELECT COUNT(&lt;%=__COLUMN_NAMES__%>) FROM &lt;%=__TABLE_NAME__%>  WHERE LTRIM(RTRIM(&lt;%=__COLUMN_NAMES__%>)) = '' &lt;%=__AND_WHERE_CLAUSE__%>" language="DB2"/>
          <sqlGenericExpression xmi:id="_la_BdpdCEemwqt0XGhOv7A" body="SELECT COUNT(&lt;%=__COLUMN_NAMES__%>) FROM &lt;%=__TABLE_NAME__%>  WHERE LTRIM(RTRIM(&lt;%=__COLUMN_NAMES__%>)) = '' &lt;%=__AND_WHERE_CLAUSE__%>" language="Microsoft SQL Server"/>
          <sqlGenericExpression xmi:id="_la_Bd5dCEemwqt0XGhOv7A" body="SELECT COUNT(&lt;%=__COLUMN_NAMES__%>) FROM &lt;%=__TABLE_NAME__%> WHERE (&lt;%=__COLUMN_NAMES__%> = '' or RTRIM(LTRIM(&lt;%=__COLUMN_NAMES__%>)) = '') &lt;%=__AND_WHERE_CLAUSE__%>" language="Adaptive Server Enterprise | Sybase Adaptive Server IQ"/>
        </builtInIndicatorDefinition>
      </indicators>
      <indicators xsi:type="dataquality.indicators:TextIndicator" xmi:id="_ki3HTZdCEemwqt0XGhOv7A">
        <parameters xmi:id="_ki3HTpdCEemwqt0XGhOv7A"/>
        <analyzedElement xsi:type="relational:TdColumn" href="../../metadata/connections/EDD_0.1.item#_lpM19oBbEemyxqr3WvsPQg"/>
        <indicatorDefinition href="../../TDQ_Libraries/Indicators/System Indicators/Text Statistics/Text_Statistics_0.1.definition#_yb9x0zh8Ed2XmO7pl5Yuyg"/>
        <averageLengthIndicator xmi:id="_ki3HT5dCEemwqt0XGhOv7A">
          <parameters xmi:id="_ki3HUJdCEemwqt0XGhOv7A">
            <textParameter xmi:id="_ki3HUZdCEemwqt0XGhOv7A" useBlank="false"/>
          </parameters>
          <analyzedElement xsi:type="relational:TdColumn" href="../../metadata/connections/EDD_0.1.item#_lpM19oBbEemyxqr3WvsPQg"/>
          <indicatorDefinition href="../../TDQ_Libraries/Indicators/System Indicators/Text Statistics/Average_Length_0.1.definition#_ccIR4BF2Ed2PKb6nEJEvhw"/>
          <builtInIndicatorDefinition xmi:id="_lbILYJdCEemwqt0XGhOv7A" name="Average Length" label="Average Length">
