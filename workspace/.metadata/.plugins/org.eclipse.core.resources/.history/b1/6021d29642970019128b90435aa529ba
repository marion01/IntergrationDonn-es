      <indicators xsi:type="dataquality.indicators:BlankCountIndicator" xmi:id="_ki3HiJdCEemwqt0XGhOv7A">
        <parameters xmi:id="_ki3HiZdCEemwqt0XGhOv7A"/>
        <analyzedElement xsi:type="relational:TdColumn" href="../../metadata/connections/EDD_0.1.item#_lpM1-oBbEemyxqr3WvsPQg"/>
