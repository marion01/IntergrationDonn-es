      <indicators xsi:type="dataquality.indicators:DuplicateCountIndicator" xmi:id="_ki3HDJdCEemwqt0XGhOv7A">
        <parameters xmi:id="_ki3HDZdCEemwqt0XGhOv7A"/>
        <analyzedElement xsi:type="relational:TdColumn" href="../../metadata/connections/EDD_0.1.item#_lpM18oBbEemyxqr3WvsPQg"/>
