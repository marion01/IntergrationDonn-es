      <indicators xsi:type="dataquality.indicators:BlankCountIndicator" xmi:id="_ki3G0ZdCEemwqt0XGhOv7A">
        <parameters xmi:id="_ki3G0pdCEemwqt0XGhOv7A"/>
        <analyzedElement xsi:type="relational:TdColumn" href="../../metadata/connections/EDD_0.1.item#_lpM10oBbEemyxqr3WvsPQg"/>
