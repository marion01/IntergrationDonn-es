          <taggedValue xmi:id="_lWN2MZdCEemwqt0XGhOv7A" tag="Description" value="counts the number of unique rows (i.e. distinct rows with only one instance: rows that are not duplicated)"/>
          <taggedValue xmi:id="_lWN2MpdCEemwqt0XGhOv7A" tag="Purpose" value="evaluates the number of unique records"/>
